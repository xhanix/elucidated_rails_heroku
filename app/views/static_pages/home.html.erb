<div id="myCarousel" class="carousel slide" data-interval="3000" data-ride="carousel" style="height: 500px;min-width: 750px;background-color: #f9f7f7;">
    <!-- Carousel indicators -->
     <ol class="carousel-indicators" >
    <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
    <li data-target="#myCarousel" data-slide-to="1"></li>
    <li data-target="#myCarousel" data-slide-to="2"></li>
  </ol> 
    <!-- Carousel items -->
    <div class="carousel-inner">
        <div class="item active">
            <img src="<%= image_url("pay_logo.png") %>" class="d-block img-fluid" alt="First Slide">
        </div>
        <div class="item">
            <img src="<%= image_url("pay_logo.png") %>" class="d-block img-fluid" alt="Second Slide">
          
        </div>
        <div class="item">
            <img src="<%= image_url("pay_logo.png") %>" class="d-block img-fluid" alt="Third Slide">
        </div>
    </div>
  
 
</div>